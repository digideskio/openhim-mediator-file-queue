{
  "port": 4002,
  "log_level": "debug",
  "endpoints": [
    {
      "name": "echoServer",
      "path": "/test",
      "url": "http://localhost:8000",
      "paused": false,
      "parallel": 2,
      "updateTx": true,
      "forwardMetadata": true
    }
  ],
  "mediatorConf": {
    "defaultChannelConfig": [
      {
        "name": "File Queue",
        "urlPattern": "^/test$",
        "status": "enabled",
        "routes": [
          {
            "name": "File queue",
            "host": "localhost",
            "path": "/test",
            "port": "4002",
            "secured": false,
            "primary": true,
            "type": "http"
          }
        ],
        "authType": "public"
      }
    ]
  }
}
